{
    "name": "movemoveapp/laravel-localization",
    "description": "Easy localization for Laravel with a support for localization switch by domain names.",
    "keywords": [
        "php",
        "laravel",
        "laravel-package",
        "laravel-localization",
        "language-selector",
        "laravel-domains",
        "laravel-domain-localization"
    ],
    "homepage": "https://github.com/movemoveapp/laravel-localization",
    "license": "MIT",
    "version": "1.7.0",
    "authors": [
        {
            "name": "Marc CÃ¡mara",
            "email": "mcamara88@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Dmitry Kovalev",
            "email": "d.kovalev@movemove.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~5.2.0||~5.3.0||~5.4.0||~5.5.0||~5.6.0||~5.7.0||~5.8.0||^6.0||^7.0||^8.0"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "~3.4|~3.8|~4.0",
        "phpunit/phpunit": "6.0.*|^8.0"
    },
    "suggest": {
        "ext-intl": "*"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-0": {
            "Mcamara\\LaravelLocalization": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mcamara\\LaravelLocalization\\LaravelLocalizationServiceProvider"
            ],
            "aliases": {
                "LaravelLocalization": "Mcamara\\LaravelLocalization\\Facades\\LaravelLocalization"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
